				prevEl : ".navigation.expected .swiper-button-prev",
				// 버튼 클릭 여부
				clickable : true,
			},
			slidesPerView : '5',
			spaceBetween : 30,
			watchOverflow : true,
			slidesPerGroup : 5,
		});
	</script>
